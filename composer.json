{
    "name": "yii-tools/contact-form",
    "type": "library",
    "description": "Contact Form for for YiiFramework v.3.0.",
    "keywords": [
        "yii-tools",
        "contact-form",
        "yii3"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "cebe/markdown": "^1.2",
        "httpsoft/http-message": "^1.0.5",
        "psr/http-message": "^1.0",
        "yii-tools/bootstrap5-asset": "^1.0@dev",
        "yii-tools/bootstrapicons-asset": "^1.0@dev",
        "yii-tools/bulma-asset": "^1.0@dev",
        "yii-tools/filepond-widget": "^1.0@dev",
        "yii-tools/fontawesome-asset": "^1.0@dev",
        "yii-tools/form-model": "^1.0@dev",
        "yii-tools/forms": "^1.0@dev",
        "yii-tools/markdowneditor-widget": "^1.0@dev",
        "yii-tools/materialize-asset": "^1.0@dev",
        "yii-tools/service": "^1.0@dev",
        "yii-tools/tailwind-asset": "^1.0@dev",
        "yiisoft/aliases": "^2.0",
        "yiisoft/http": "^1.2",
        "yiisoft/yii-view": "^5.0"
    },
    "require-dev": {
        "codeception/c3": "^2.7",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-cli": "^2.0",
        "codeception/module-phpbrowser": "^3.0",
        "maglnet/composer-require-checker": "^4.3",
        "roave/infection-static-analysis-plugin": "^1.28",
        "vimeo/psalm": "^5.6",
        "yiisoft/router-fastroute": "^2.0",
        "yiisoft/router": "^2.0",
        "yiisoft/yii-middleware": "dev-master",
        "yiisoft/yii-runner-http": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yii\\ContactForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\ContactForm\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "application-params": "?application-params.php",
            "params": "params.php",
            "routes": "routes.php",
            "widgets": "widgets.php"
        },
        "config-plugin-environments": {
            "test": {
                "bootstrap": [
                    "?bootstrap.php"
                ],
                "bootstrap-console": [
                    "$bootstrap"
                ],
                "bootstrap-web": [
                    "$bootstrap"
                ],
                "common": [
                    "../tests/Support/Data/config/common/*.php"
                ],
                "params": "../tests/Support/Data/config/params.php",
                "routes": "routes.php",
                "web": [
                    "$common",
                    "../tests/Support/Data/config/web/*.php"
                ]
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "foxy/foxy": true,
            "codeception/c3": true,
            "yiisoft/config": true
        }
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "mutation": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "test": "codecept run"
    }
}
